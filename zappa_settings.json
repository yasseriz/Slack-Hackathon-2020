{
    "prod": {
      "app_function": "waterBot.app",
      "aws_region": "ap-southeast-1",
      "exclude": [
        "__pycache__",
        ".git/*",
        ".gitignore",
        ".python-version",
        "LICENSE",
        "README.md",
        "requirements.txt",
        "zappa_settings.json",
      ],
      "keep_warm": true,
      "keep_warm_expression": "rate(5 minutes)",
      "memory_size": 128,
      "profile_name": "default",
      "project_name": "waterBot",
      "runtime": "python3.6",
      "timeout_seconds": 30,
    }
  }